  "private": true,
  "dependencies": {
    "axios": "1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.15.0",
    "react-scripts": "5.0.1",
    "styled-components": "6.0.7",
    "web-vitals": "2.1.4"
  },
  "scripts": {
